						"sqlReaderQuery": "SELECT CONCAT('''',CONVERT(varchar(50), max_created_at),'''') as max_created_at,\n CONCAT('''',CONVERT(varchar(50), max_updated_at),'''') as max_updated_at\n FROM(\nSELECT MAX(FORMAT(created_at,'yyyy-MM-dd HH:mm:ss')) as max_created_at,  MAX(FORMAT(updated_at,'yyyy-MM-dd HH:mm:ss')) as max_updated_at FROM stg.\"Agencies\") a",

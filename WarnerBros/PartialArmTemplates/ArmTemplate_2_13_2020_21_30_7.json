{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WarnerBros"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CETA_WB')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CETA",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"start": {
						"type": "string",
						"defaultValue": "2018/12/23"
					},
					"end": {
						"type": "string",
						"defaultValue": "2019/6/23"
					},
					"source": {
						"type": "string",
						"defaultValue": "FOX1"
					},
					"token": {
						"type": "string",
						"defaultValue": "ABC"
					},
					"key": {
						"type": "string",
						"defaultValue": "12345"
					}
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "https://dms.cetacloud.tv/dmsapi/index.php/query/index/?start=2019/12/1&end=2020/12/31&source=WARNER1&token=ABC&key=12345",
					"requestMethod": "GET"
				}
			},
			"dependsOn": []
		}
	]
}